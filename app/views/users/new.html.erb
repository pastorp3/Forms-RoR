 <h1>Welcome this is a test</h1>

<%= form_tag('/users') do %>
  <%= label_tag(:username) %>
  <br>
  <%= text_field_tag(:username, nil, placeholder: 'Enter your user name') %>
  <br>
  <br>
  <%= label_tag(:email) %>
  <br>
  <%= email_field_tag(:email, nil, placeholder: 'Enter your email here')%>
  <br>
  <br>
  <%= label_tag(:password) %>
  <br>
  <%= password_field_tag(:password)%>
  <br>
  <br>
  <%= submit_tag("Add user")%>
<% end %>
